@{
    ViewData["Title"] = "Account";
    var cookies = Context.Request.Cookies;
}

<div class="container">
    <h1>Account Information</h1>

    <div class="card mt-3">
        <h3>Cookie Values</h3>
        <ul>
            @foreach (var c in cookies)
            {
                <li>@c.Key : @c.Value</li>
            }
        </ul>
    </div>
</div>
